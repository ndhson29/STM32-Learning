#include <stdint.h>

int main(void)
{
	uint32_t *pClkCtrlReg 	= (uint32_t*)0x40023830;
	uint32_t *pPortDModeReg = (uint32_t*)0x40020C00;
	uint32_t *pPortDOutReg 	= (uint32_t*)0x40020C14;

	/* 1. Enable the clock for peripheral in the AHB1ENR (Set the 3rd bit position */
	*pClkCtrlReg |= ( 1 << 3 );

	/* 2. Configure the mode of the IO pin as output  */
	/* a. Clear bit 24th and 25th position */
	*pPortDModeReg &= ~( 3 << 24 );

	/* b. Make 24th bit position as 1 (SET) */
	*pPortDoutReg |= ( 1 << 24 );

    /* Loop forever */
	for(;;);
}
